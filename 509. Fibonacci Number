class Solution {
    
    private int[] dp;
    
    int fi(int n){
        if (n==0 || n==1){
            return n;
        }
        if (dp[n]>0){
            return dp[n];
        }
        dp[n] = fi(n-1) + fi(n-2);
        return dp[n];
    }
    
    public int fib(int n) {
        dp = new int[n+1];
        for(int i=0; i<=n ;i++){
            dp[i] = -1;
        }
        if (n==0 || n==1){
            return n;
        }
        dp[0] = 0;
        dp[1] = 1;
        return fi(n-1) + fi(n-2);
    }
}
