import java.util.Arrays;
class Solution {
    public void reverseString(char[] s) {
        int length = s.length;
        int j = length-1;
        int k = length/2;
        for(int i=0; i < k; i++){
            char temp = s[i];
            s[i]= s[j];
            s[j]=temp;
            j--;
        }
    }
}
