class Solution {
    public String toLowerCase(String s) {
        String ans = "";
        for (int i=0;i<s.length();i++){
            if (s.charAt(i)>='a' && s.charAt(i)<='z'){
                ans+=s.charAt(i);
            }else {
                ans += Character.toLowerCase(s.charAt(i));
            }
        }
        return ans;
    }
    
}
